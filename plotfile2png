set title "Kellertemperatur in °C"
set xlabel "Zeit"
set ylabel "Temperatur in °C"

set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"
#set xrange["2016-06-09 11:00:00":"2016-06-09 14:10:00"]
set yrange[*:*]
set autoscale x
set datafile separator ';'
set grid xtics ytics
set term png
set output "temperaturAll.png"
#show xrange
plot 'datefile.txt' using 1:2 title "Temperatur (alles)" with lines lt rgb "#0000FF" lw 3

