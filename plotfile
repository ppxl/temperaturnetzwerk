set title "Kellertemperatur in °C"
#unset multiplot
set xlabel "Zeit"
set ylabel "Temperatur in °C"
set xdata time
set timefmt "%Y-%m-%d %H:%M:%S"
set xrange["2016-06-09 11:00:00":"2016-06-09 14:10:00"]
set yrange[*:*]
set autoscale x
set style line 1 lc rgb '#0060ad' lt 1 lw 3 pt 7 ps 1.5
set datafile separator ';'
set grid xtics ytics
#set term png
#set output "temperatur.png"
#show xrange
set key outside
#plot for [col=2:3] 'datefile.txt' using 1:col with lines
plot 'datefile.txt' using 1:2 title "Temperatur" with lines, \
	'datefile.txt' using 1:3 title "Durchschnitt" with lines
